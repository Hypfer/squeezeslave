svn diff -r34 Slim/Display/Text.pm >> ${PATCHFILE}
svn diff -r34 Slim/Display/Lib/TextVFD.pm >> ${PATCHFILE}
cp -ip ${PATCHFILE} ../squeezeslave/patches
